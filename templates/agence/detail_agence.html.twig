{% extends 'base.html.twig' %}
{% block title %} {{ parent()}} Agence | Detail{% endblock %}

{% block body %}

<div class="row justify-content-end">
    <a class="btn btn-info p-2 col-md-3" href="{{ path('app_agence') }}">Retour</a>
</div>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Agence</th>
      <th scope="col">Titre</th>
      <th scope="col">Adresse</th>
      <th scope="col">Ville</th>
      <th scope="col">CP</th>
      <th scope="col">Description</th>
      <th scope="col">Photo</th>
     
    </tr>

  </thead>

  <tbody>

    <tr>
      <td>{{ agence.id }}</td>
      <td>{{ agence.titre }}</td>
      <td>{{ agence.adresse }}</td>
      <td>{{ agence.ville }}</td>
      <td>{{ agence.cp }}</td>
      <td>{{ agence.description }}</td>
      <td> 
      <img src="{{ agence.photo }}" alt="" width=150 height=100>
      </td>
   
    </tr>
  </tbody>
</table>


{% endblock %}